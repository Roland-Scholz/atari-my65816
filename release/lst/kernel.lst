00F895  2               OP_OR    = '|'      ; Bit-wise OR operator
00F895  2               linbuf   = $0380    ; input line buffer
00F895  2               prgm     = $1000    ; VTL02B program grows from here
00F895  2               himem    = $C000    ;   ... up to the top of user RAM
